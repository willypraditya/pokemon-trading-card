{
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true,
    "importOrder": [
        "^react$",
        "^next(.*)$",
        "<THIRD_PARTY_MODULES>",
        "@/components/(.*)$",
        "@/utils/(.*)$",
        "^./(.*)$",
        "^../(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
